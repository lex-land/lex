{
  "processors": [
    "stylelint-processor-styled-components"
  ],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
  "rules": {
    "indentation": 2,
    "string-quotes": "single",
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "extends"
        ]
      }
    ],
    "block-no-empty": null,
    "font-family-no-missing-generic-family-keyword": null,
    "no-descending-specificity": null
  }
}
