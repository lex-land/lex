{
  "private": true,
  "author": " <郭永杰 guoyongjie@sunmi.com>",
  "license": "MIT",
  "name": "lex",
  "version": "1.0.0",
  "description": "sunmi,shopbi,next",
  "main": "server/main.ts",
  "repository": "git@github.com:sunmi-web/lex.git",
  "keywords": [
    "sunmi"
  ],
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "prebuild": "rimraf build",
    "build": "npm run build:next",
    "predatabase": "docker-compose -f ./docker/docker-compose.database.yml down",
    "database": "docker-compose -f ./docker/docker-compose.database.yml up -d",
    "build:next": "npx next build",
    "build:local": "SUNMI_PROD_URL=http://localhost:3000 npm run build",
    "build:master": "SUNMI_PROD_URL=https://lex-land.onlone SUNMI_ENV=master npm run build",
    "start": "NODE_ENV=production npx ts-node --require tsconfig-paths/register --require reflect-metadata server/main.ts",
    "start:dev": "SUNMI_PROD_URL=http://localhost:3000 npx nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "test:watch": "jest --watch",
    "predeploy": "docker-compose -f ./docker/docker-compose.yml pull && docker-compose -f ./docker/docker-compose.yml down",
    "deploy": "docker-compose -f ./docker/docker-compose.yml up -d",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test": "jest --forceExit --coverage --verbose",
    "debug": "npm run build && npm run watch-debug"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.15.1",
    "@blueprintjs/select": "^3.8.1",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "csurf": "^1.10.0",
    "express": "^4.16.4",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.1",
    "formik": "^1.5.4",
    "helmet": "^3.18.0",
    "isomorphic-fetch": "^2.2.1",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mysql": "^2.14.1",
    "next": "^8.1.0",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path-to-regexp": "^3.0.0",
    "pluralize": "^7.0.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scrollspy": "^3.4.0",
    "react-sticky": "^6.0.3",
    "react-use": "^9.0.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "^6.3.3",
    "typeorm": "0.2.17",
    "universal-cookie": "^4.0.0",
    "xlsx": "^0.14.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@nestjs/testing": "^6.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/classnames": "^2.2.7",
    "@types/crypto-js": "^3.1.43",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.16.1",
    "@types/express-rate-limit": "^3.3.0",
    "@types/express-session": "^1.15.12",
    "@types/form-data": "^2.2.1",
    "@types/helmet": "^0.0.43",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.126",
    "@types/md5": "^2.1.33",
    "@types/next": "^8.0.5",
    "@types/node": "^8.0.29",
    "@types/nprogress": "0.0.29",
    "@types/numeral": "0.0.25",
    "@types/passport-jwt": "^3.0.1",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.5.3",
    "@types/react-scrollspy": "^3.3.0",
    "@types/react-sticky": "^6.0.3",
    "@types/react-wait": "^0.3.0",
    "@types/shelljs": "^0.8.5",
    "@types/styled-components": "^4.1.14",
    "@types/supertest": "^2.0.7",
    "@types/yup": "^0.26.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "^8.1.6",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "prettier-stylelint": "^0.4.2",
    "rebound": "^0.1.0",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.3",
    "styled-components": "^4.2.0",
    "stylelint": "^9.10.1",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "3.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "3.3.3333",
    "webpack": "4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "useJSXTextNode": true,
      "project": "tsconfig.json"
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "google",
      "prettier/@typescript-eslint",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "sort-imports": [
        "error",
        {
          "ignoreCase": false,
          "ignoreDeclarationSort": false,
          "ignoreMemberSort": false,
          "memberSyntaxSortOrder": [
            "none",
            "all",
            "multiple",
            "single"
          ]
        }
      ],
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-parameter-properties": "off",
      "@typescript-eslint/explicit-member-accessibility": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/camelcase": [
        "error",
        {
          "properties": "never",
          "ignoreDestructuring": true
        }
      ],
      "no-invalid-this": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "spaced-comment": "off",
      "camelcase": "off",
      "react/prop-types": "off",
      "react/display-name": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "require-jsdoc": "off",
      "valid-jsdoc": "warn",
      "new-cap": "off"
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "import",
        {
          "libraryName": "lodash",
          "libraryDirectory": "",
          "camel2DashComponentName": false
        },
        "lodash"
      ],
      [
        "import",
        {
          "libraryName": "react-use",
          "libraryDirectory": "lib",
          "camel2DashComponentName": false
        },
        "react-use"
      ],
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "@components": "./components",
            "@core": "./core",
            "@config": "./config",
            "@helpers": "./helpers",
            "@server": "./server",
            "@layout": "./layout"
          },
          "extensions": [
            ".ts",
            ".tsx"
          ]
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "nodemonConfig": {
    "watch": [
      "next.config.js",
      "server",
      "config",
      "core",
      "helpers"
    ],
    "verbose": false,
    "debug": false,
    "ext": "ts",
    "exec": "node --require ts-node/register --require tsconfig-paths/register --require reflect-metadata server/main.ts",
    "ignore": [
      "node_modules",
      "**/*.d.ts",
      "*.test.ts",
      "*.spec.ts"
    ],
    "inspect": false
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-idiomatic-order",
      "./node_modules/prettier-stylelint/config.js"
    ],
    "rules": {
      "indentation": 2,
      "string-quotes": "single",
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "extends"
          ]
        }
      ],
      "block-no-empty": null,
      "font-family-no-missing-generic-family-keyword": null,
      "no-descending-specificity": null
    }
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "eslint"
      ],
      "*.{css,less}": [
        "stylelint"
      ]
    },
    "ignore": [
      "node_modules",
      "**.d.ts"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "testRegex": "tests/*.spec.ts$",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@components/(.*)$": "<rootDir>/components/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@helpers/(.*)$": "<rootDir>/helpers/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
