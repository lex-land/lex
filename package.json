{
  "private": true,
  "version": "1.0.0",
  "name": "lex",
  "description": "A Collaboration Tool for API documentation",
  "main": "pages/index",
  "repository": "git@github.com:lex-land/lex.git",
  "author": "briefguo <gyj9094@gmail.com>",
  "keywords": [
    "react",
    "nextjs",
    "lex",
    "docker",
    "rap"
  ],
  "license": "MIT",
  "scripts": {
    "prestart:server": "docker-compose -f ./docker/server.yml down --remove-orphans",
    "start:server": "docker-compose -f ./docker/server.yml up -d",
    "docker:build": "docker build --tag lexland/lex:latest .",
    "docker:push": "docker push lexland/lex:latest",
    "start:dev": "npx next dev",
    "start": "npx next start",
    "prebuild": "rimraf build",
    "build": "npx next build"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.26.0",
    "@blueprintjs/select": "^3.12.2",
    "class-validator": "^0.12.2",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "formik": "^2.1.4",
    "highlight.js": "^10.0.1",
    "isomorphic-fetch": "^2.2.1",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "next": "^9.3.6",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^6.1.0",
    "pluralize": "^8.0.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scrollspy": "^3.4.2",
    "react-sticky": "^6.0.3",
    "react-use": "^14.2.0",
    "rxjs": "^6.5.5",
    "styled-components": "^5.1.0",
    "typeorm": "0.2.24",
    "universal-cookie": "^4.0.3",
    "xlsx": "^0.15.6",
    "yup": "^0.28.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/classnames": "^2.2.10",
    "@types/crypto-js": "^3.1.45",
    "@types/highlight.js": "^9.12.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/lodash": "^4.14.150",
    "@types/md5": "^2.2.0",
    "@types/node": "^13.13.4",
    "@types/nprogress": "0.2.0",
    "@types/numeral": "0.0.27",
    "@types/pluralize": "0.0.29",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.34",
    "@types/react-scrollspy": "^3.3.2",
    "@types/react-sticky": "^6.0.3",
    "@types/react-wait": "^0.3.1",
    "@types/styled-components": "^5.1.0",
    "@types/supertest": "^2.0.8",
    "@types/yup": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "prettier-stylelint": "^0.4.2",
    "react-is": "^16.13.1",
    "rebound": "^0.1.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "stylelint": "^13.3.3",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.8.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "stylelint",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "testRegex": "tests/*.spec.ts$",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@components/(.*)$": "<rootDir>/components/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@helpers/(.*)$": "<rootDir>/helpers/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
